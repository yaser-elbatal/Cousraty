{
  "expo": {
    "name": "البرنامج التربوي الفردي",
    "slug": "tarbawi",
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#2E234D",
      "translucent": false
    },
    "version": "0.0.9",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.awmer.tarbawi",
      "userInterfaceStyle": "light",
      "infoPlist": {
        "NSCameraUsageDescription": "App need this permission to update profile photo.",
        "NSPhotoLibraryAddUsageDescription": "App need this permission to update profile photo.",
        "NSLocationWhenInUseUsageDescription": "App need this permission to return the nearest providers.",
        "NSPhotoLibraryUsageDescription": "App need this permission to update profile photo."
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "com.awmer.tarbawis",
      "versionCode": 9,
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "CAMERA_ROLL",
        "READ_PHONE_STATE",
        "NOTIFICATIONS",
        "MEDIA_LIBRARY",
        "MEDIA_LIBRARY_WRITE_ONLY" ,
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "READ_INTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"],
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBQu-aDY--uY14s4U3ok-XwwqhsnhLFe5Q"
        }
      }
    }
  }
}
